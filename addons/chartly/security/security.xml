<odoo>
    <data>
        <record id="group_openai_chat_user" model="res.groups">
            <field name="name">OpenAI Chat User</field>
        </record>
        <record id="group_openai_chat_manager" model="res.groups">
            <field name="name">OpenAI Chat Manager</field>
        </record>
        <record id="access_openai_settings" model="ir.model.access">
            <field name="name">openai.settings access</field>
            <field name="model_id" ref="model_openai_settings"/>
            <field name="group_id" ref="group_openai_chat_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="0"/>
            <field name="perm_create" eval="0"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="access_chat_conversation" model="ir.model.access">
            <field name="name">chat.conversation access</field>
            <field name="model_id" ref="model_openai_chat_conversation"/>
            <field name="group_id" ref="group_openai_chat_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
        <record id="access_chat_message" model="ir.model.access">
            <field name="name">chat.message access</field>
            <field name="model_id" ref="model_openai_chat_message"/>
            <field name="group_id" ref="group_openai_chat_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
    </data>
</odoo>